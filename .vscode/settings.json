{
	"editor.tabSize": 1,
	"editor.insertSpaces": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[svelte]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"[xml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"editor.formatOnSave": true,
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"[shellscript]": {
		"editor.defaultFormatter": "mkhl.shfmt",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"files.associations": {
		"*.svg": "xml"
	}
}
