{
	"editor.tabSize": 1,
	"editor.insertSpaces": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[svelte]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"[xml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 1,
		"editor.insertSpaces": false
	},
	"editor.formatOnSave": true,
	"files.associations": {
		"*.svg": "xml"
	}
}
