{
	"$schema": "./gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		{
			"identifier": "shell:allow-spawn",
			"allow": [
				{
					"name": "binaries/lish-backend",
					"sidecar": true,
					"args": true
				}
			]
		},
		"shell:allow-kill"
	]
}
